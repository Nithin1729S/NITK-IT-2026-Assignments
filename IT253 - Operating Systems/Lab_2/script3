# !/bin/bash

if [ $# -eq 3 ]
then
        if [ $1 -ge $2 ] && [ $1 -ge $3 ]
	then
    		echo "The greatest of three number entered is $1"
    		
	elif [ $2 -ge $1 ] && [ $2 -ge $3 ]
	then
   		echo "The greatest of three number entered is $2"
   		
	elif [ $3 -gt $1 ] && [ $3 -gt $1 ]
        then
    		echo "The greatest of three number entered is $3"
fi
else
        echo "ERROR:You have not entered three numbers"
fi

